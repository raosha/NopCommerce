
@model Nop.Plugin.Misc.Ebay.Models.FileUploadViewModel
@{
    //page title
    ViewBag.PageTitle = "Delivery Labels";
    Layout = "_AdminLayout";
    Html.SetActiveMenuItemSystemName("Delivery Labels");
}


<div class="content">
    <div class="form-horizontal">
        <div class="panel-group">
            <div class="panel panel-default">
                <div class="panel-body">
                    <form enctype="multipart/form-data" method="post" asp-action="Create" asp-controller="DeliveryLabel">
                        <div class="form-group">
                            <div class="col-md-3">
                                <nop-label asp-for="Title" />
                            </div>
                            <div class="col-md-9">
                                <nop-editor asp-for="Title" asp-required="true" asp-value="@Model.Title" />
                                <span asp-validation-for="Title"></span>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-3">
                                <nop-label asp-for="FormFile" />
                            </div>
                            <div class="col-md-9">
                                <nop-editor asp-for="FormFile" asp-required="true" asp-value="@Model.FormFile" />
                                <span asp-validation-for="FormFile"></span>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-9 col-md-offset-3">
                                <input asp-page-handler="Upload" class="btn btn-primary" type="submit" value="Upload" />
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>

    </div>
</div>

